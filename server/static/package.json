{
  "name": "polymer-starter-kit",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-html": "^1.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "wct-browser-legacy": "^1.0.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit -p tsconfig.strict.json",
    "type-check:strict-migration": "tsc --noEmit -p tsconfig.strict-migration.json",
    "lint": "eslint . --ext js,html,ts --ignore-path .gitignore",
    "test": "npm run lint && polymer test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "HEADED=1 playwright test",
    "test:e2e:report": "playwright show-report"
  },
  "version": "",
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  },
  "author": "The Polymer Authors",
  "dependencies": {
    "@material/web": "^2.2.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "firebase": "^11.1.0",
    "lit": "^3.2.1",
    "pwa-helpers": "^0.9.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "reselect-tools": "0.0.7",
    "web-animations-js": "^2.3.2"
  }
}
